<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="io.noties.markwon.sample">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:label="@string/app_name"
        android:theme="@style/AppTheme"
        tools:ignore="AllowBackup,GoogleAppIndexingWarning,MissingApplicationIcon">

        <activity android:name="io.noties.markwon.sample.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity android:name="io.noties.markwon.sample.core.CoreActivity" />
        <activity android:name="io.noties.markwon.sample.latex.LatexActivity" />
        <activity android:name="io.noties.markwon.sample.customextension.CustomExtensionActivity" />
        <activity android:name="io.noties.markwon.sample.basicplugins.BasicPluginsActivity" />
        <activity android:name="io.noties.markwon.sample.recycler.RecyclerActivity" />
        <activity android:name="io.noties.markwon.sample.theme.ThemeActivity" />
        <activity android:name=".html.HtmlActivity" />
        <activity android:name=".simpleext.SimpleExtActivity" />
        <activity android:name=".customextension2.CustomExtensionActivity2" />

    </application>

</manifest>